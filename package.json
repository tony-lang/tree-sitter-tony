{
  "name": "tree-sitter-tony",
  "version": "0.2.0-alpha.15",
  "description": "Tony grammar for tree-sitter",
  "main": "index.js",
  "types": "tree-sitter-tony.d.ts",
  "scripts": {
    "setup": "yarn global add tree-sitter-cli && yarn generate && yarn install --dev",
    "generate": "tree-sitter generate",
    "parse": "tree-sitter parse",
    "test": "tree-sitter test",
    "types": "node ./node_modules/@asgerf/dts-tree-sitter/build/src . > tree-sitter-tony.d.ts",
    "build": "yarn generate && yarn types",
    "lint": "prettier --check *.js",
    "prettier": "prettier --write *.js",
    "install": "prebuild-install || node-gyp rebuild"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:tony-lang/tree-sitter-tony.git"
  },
  "keywords": [
    "tree-sitter",
    "grammar",
    "langugage"
  ],
  "author": "Jonas HÃ¼botter",
  "license": "LGPL-3.0",
  "bugs": {
    "url": "https://github.com/tony-lang/tree-sitter-tony/issues"
  },
  "homepage": "https://github.com/tony-lang/tree-sitter-tony",
  "dependencies": {
    "@asgerf/dts-tree-sitter": "^0.1.0",
    "nan": "^2.14.1",
    "prebuild-install": "^6.0.0"
  },
  "devDependencies": {
    "prebuild": "^10.0.0",
    "prettier": "^2.0.5",
    "tree-sitter": "^0.17.0",
    "tree-sitter-cli": "^0.17.1",
    "typescript": "^4.0.2"
  }
